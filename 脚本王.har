            ],
            "failed_asserts": [
                {
                    "re": "用户名或密码错误",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "dlzt",
                    "re": "\"msg\":\"(.+)\"",
                    "from": "content"
                }
            ]
        }
    },
